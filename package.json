{
  "name": "yarle-evernote-to-md",
  "version": "3.5.3",
  "description": "Yet Another Rope Ladder from Evernote",
  "keywords": [
    "evernote",
    "enex",
    "converter",
    "md",
    "markdown",
    "enex to md"
  ],
  "main": "./dist/dropTheRope.js",
  "bin": {
    "yarle": "./dist/dropTheRope.js"
  },
  "scripts": {
    "build": "tsc --project ./src/tsconfig.json",
    "build:watch": "nodemon --watch src/ --exec \"npm run build\" -e ts",
    "coverage": "nyc mocha",
    "lint": "tslint --project ./src/tsconfig.json",
    "start": "node --max-old-space-size=1024 ./dist/dropTheRope.js",
    "debug": "node --inspect --max-old-space-size=1024 ./dist/dropTheRope.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "test-watch": "mocha --watch"
  },
  "author": "Akos Balasko",
  "dependencies": {
    "@types/fs-extra": "8.1.0",
    "chai": "4.2.0",
    "fast-xml-parser": "3.12.5",
    "fs-extra": "4.0.2",
    "he": "1.2.0",
    "joplin-turndown-plugin-gfm": "1.0.12",
    "jsdom": "16.4.0",
    "lodash": "4.17.19",
    "marked": "1.2.0",
    "md5-file": "5.0.0",
    "mime-types": "2.1.28",
    "moment": "2.22.2",
    "utimes": "4.0.3",
    "proxyquire": "2.1.3",
    "sanitize-filename": "^1.6.3",
    "ts-node": "8.5.4",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-microsoft-contrib": "6.2.0",
    "turndown": "7.0.0",
    "typescript": "3.8.3",
    "xml-flow": "1.0.4"
  },
  "engines": {
    "node": ">=10.18.1",
    "npm": ">=5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akosbalasko/yarle"
  },
  "homepage": "https://github.com/akosbalasko/yarle",
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "execSync": "1.0.2",
    "@types/he": "1.1.1",
    "@types/jsdom": "16.2.4",
    "@types/lodash": "4.14.151",
    "@types/marked": "1.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.1",
    "@types/proxyquire": "1.3.28",
    "@types/showdown": "1.9.3",
    "@types/sinon": "9.0.1",
    "mocha": "5.2.0",
    "nodemon": "2.0.5",
    "nyc": "13.0.1",
    "sinon": "9.0.2",
    "timezone-mock": "1.1.1"
  }
}
